{
    "manifest_version": 2,
    "name": "Search Engine Tabs",
    "version": "0.1",
    "permissions": ["tabs", "http://*.google.com/*", "https://*.google.com/*",
                    "http://duckduckgo.com/*", "https://duckduckgo.com/*",
                    "http://bing.com/*", "https://bing.com/*"],
    "content_scripts": [
        {
          "matches": ["http://*.google.com/*", "https://*.google.com/*",
            "http://duckduckgo.com/*", "https://duckduckgo.com/*",
            "http://bing.com/*", "https://bing.com/*"],
          "js": ["script.js"],
          "css": ["styles.css"],
          "run_at": "document_end"
        }
    ]
  }